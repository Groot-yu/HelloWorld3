<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yyg.tiange">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.yyg.tiange.datastorage.ExternalStorageActivity"></activity>
        <activity android:name="com.yyg.tiange.datastorage.FileActivity" />
        <activity android:name="com.yyg.tiange.datastorage.SharedPerencesActivity" />
        <activity android:name="com.yyg.tiange.datastorage.DataStorageActivity" />
        <activity android:name="com.yyg.tiange.IntentActivity">
            <intent-filter>
                <action android:name="yinshi" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="yinshi" />
            </intent-filter>
        </activity>
        <activity android:name="com.yyg.tiange.MenuActivity" />
        <activity android:name="com.yyg.tiange.HandlerActivity" />
        <activity android:name="com.yyg.tiange.EventActivity" />
        <activity android:name="com.yyg.tiange.fragment.ContainerActivity" />
        <activity android:name="com.yyg.tiange.PopupWindowActivity" />
        <activity android:name="com.yyg.tiange.CustomDialogActivity" />
        <activity android:name="com.yyg.tiange.ProgressActivity" />
        <activity android:name="com.yyg.tiange.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.yyg.tiange.DialogActivity" />
        <activity android:name="com.yyg.tiange.ToastActivity" />
        <activity android:name="com.yyg.tiange.WebViewActivity" />
        <activity android:name="com.yyg.tiange.recyclerview.PubuRecyclerViewActivity" />
        <activity android:name="com.yyg.tiange.recyclerview.GridRecyclerViewActivity" />
        <activity android:name="com.yyg.tiange.recyclerview.HorizontalRecyclerViewActivity" />
        <activity android:name="com.yyg.tiange.listview.ListViewActivity" />
        <activity android:name="com.yyg.tiange.gridview.GridViewActivity" />
        <activity android:name="com.yyg.tiange.recyclerview.LinearRecyclerViewActivity" />
        <activity android:name="com.yyg.tiange.ImageViewActivity" />
        <activity android:name="com.yyg.tiange.CheckBoxActivity" />
        <activity android:name="com.yyg.tiange.RadioButtonActivity" />
        <activity android:name="com.yyg.tiange.EditTextActivity" />
        <activity
            android:name="com.yyg.tiange.ButtonActivity"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.yyg.tiange.TextViewActivity" />
        <activity
            android:name="com.yyg.tiange.UiActivity"
            android:label="UI" />
        <activity android:name="com.yyg.tiange.LifeCycleActivity" />
        <activity
            android:name="com.yyg.tiange.jump.AActivity"
            android:label="A" />
        <activity
            android:name="com.yyg.tiange.jump.BActivity"
            android:label="B">
            <intent-filter>

                <!-- action自定义 随便写 隐式跳转 -->
                <action android:name="implicitjump" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>